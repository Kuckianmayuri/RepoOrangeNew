<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="OrangeHRM Parallel Suite" parallel="tests"
	thread-count="3">

	<listeners>
		<listener class-name="listeners.Listener" />
	</listeners>

	<test name="Chrome Test">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.crm.testscripts.CreateEmployeeTest" />
		</classes>
	</test>

	<test name="Edge Test">
		<parameter name="browser" value="edge" />
		<classes>
			<class name="com.crm.testscripts.CreateEmployeeTest" />
		</classes>
	</test>

</suite>